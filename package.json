{
  "name": "react-starter",
  "version": "1.0.1",
  "description": "An Isomorphic React Starter",
  "main": "index.js",
  "scripts": {
    "predev": "npm run build",
    "dev": "npm-run-all -p dev:*",
    "build": "npm-run-all -p build:*",
    "start": "node dist/server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:coverage": "jest --no-cache --coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "lint": "eslint src",
    "components": "start-storybook -p 9999",
    "create:component": "componentinator",
    "create:view": "componentinator type=view",
    "create:container": "componentinator type=container",
    "build:client": "better-npm-run build:client",
    "build:server": "better-npm-run build:server",
    "dev:client": "better-npm-run dev:client",
    "dev:server": "better-npm-run dev:server"
  },
  "betterScripts": {
    "dev:client": {
      "command": "webpack-dev-server -d --quiet --no-info --hot --inline --history-api-fallback --config webpack.client.babel.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "dev:server": {
      "command": "node dist/server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:client": {
      "command": "webpack --quiet --no-info --config webpack.client.babel.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:server": {
      "command": "webpack --quiet --no-info --config webpack.server.babel.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "author": "Jared Anderson",
  "license": "MIT",
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "autoprefixer": "^7.2.5",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.2.1",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-require": "^3.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.5.0",
    "css-loader": "^0.26.4",
    "enzyme": "^2.9.1",
    "eslint": "^4.16.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^1.1.6",
    "happypack": "^4.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "json-loader": "^0.5.4",
    "normalize.css": "^7.0.0",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.2",
    "react-addons-test-utils": "^15.0.1",
    "react-component-inator": "^1.0.2",
    "react-test-renderer": "^15.6.1",
    "sinon": "^4.2.1",
    "style-loader": "^0.19.1",
    "webpack": "^1.12.9",
    "webpack-babel-jest": "^1.0.4",
    "webpack-dev-server": "^1.14.0",
    "webpack-node-externals": "^1.2.0"
  },
  "dependencies": {
    "@tuxsudo/b64": "^1.0.1",
    "better-npm-run": "0.1.0",
    "body-parser": "^1.17.1",
    "compression": "^1.6.1",
    "cors": "^2.7.1",
    "coveralls": "^3.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.13.4",
    "helmet": "^3.4.0",
    "html-minifier": "^3.3.0",
    "is-in-browser": "^1.0.2",
    "isomorphic-fetch": "^2.2.1",
    "join-classnames": "^1.0.0",
    "npm-run-all": "^4.0.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.0.3",
    "react-redux": "^5.0.2",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.7",
    "redux": "^3.4.0",
    "redux-thunk": "^2.0.1",
    "string-hash": "^1.1.0"
  }
}
